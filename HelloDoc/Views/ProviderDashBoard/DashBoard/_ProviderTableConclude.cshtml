@* ---------------table---------------------------- *@
@{
    var color = "";
    var requestor = "";

}

<table class="table text-center table-responsive-lg" id="table">



    <thead class="table-secondary">
        <tr class="fs-5">
            <th scope="col">Name</th>
            <th scope="col"></th>
            <th scope="col">Date of Birth</th>
            <th scope="col">Physician Name</th>
            <th scope="col">Date Of Service</th>
            <th scope="col">Phone</th>
            <th scope="col">Address</th>
            <th scope="col">Chatwith</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody class="fs-5">
        @foreach (var item in Model)
        {

            switch (item.requesttypeid)
            {
                case 1:
                    color = "#5FBC61";
                    requestor = "Patient";
                    break;
                case 2:
                    color = "#ED9124";
                    requestor = "Family/Friend";
                    break;
                case 3:
                    color = "#067CBD";
                    requestor = "Concierge";
                    break;
                case 4:
                    color = "#DC647E";
                    requestor = "BusinessMan";
                    break;
                default:
                    color = "yellow";
                    break;
            }


            @*   <tr style="background-color:#5FBC61 !important; color:white !important;"> *@
            <tr class="text-light" style="background-color:@color">
                <td class="my-auto">
                    @item.Name
                </td>
                <td>
                    <a href="Mailto:@item.Email">
                        <button class="btn border rounded text-white">
