            localStorage.setItem('textspanState', '(pending)');
            localStorage.setItem('triangleDisplayState', 'none');
            localStorage.setItem('DisplayState', 'block');
            localStorage.setItem('BorderStyle', '20px solid #0dcaf0');
            localStorage.setItem('id', '#trapending');

            setStylesFromLocalStorage();

        });

        $("#Active").click(function () {
                partialCurrentview = "DashBoard/_ProviderTableActive";
            currentstatus = [4, 5];
            setDataInLocalStorage("currentstatus", JSON.stringify(currentstatus));
            setDataInLocalStorage("partialCurrentview", partialCurrentview);
