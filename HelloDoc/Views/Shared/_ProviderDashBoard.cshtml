<script>
    $(document).ready(function () {

        var currentpage = 1;
        var pagesize = 4;

        // Function to retrieve data from local storage
        function getDataFromLocalStorage(key) {
            return localStorage.getItem(key);
        }

        // Function to store data in local storage
        function setDataInLocalStorage(key, value) {
            localStorage.setItem(key, value);
        }

        function setStylesFromLocalStorage() {

            // Get the stored state from local storage
            var textSpanState = localStorage.getItem('textspanState');
            var triangleDisplayState = localStorage.getItem('triangleDisplayState');
            var DisplayState = localStorage.getItem('DisplayState');
            var BorderStyle = localStorage.getItem('BorderStyle');
            var ida = localStorage.getItem('id');

            // Set the text and styles based on the stored state
            if (textSpanState) {
                $('#textspan').text(textSpanState);
            }
            if (triangleDisplayState) {
                $('.triangle').css('display', triangleDisplayState);
            }
            if (DisplayState) {

                $(ida).css('display', DisplayState);

            }
            if (BorderStyle) {
                $(ida).css('border-top', BorderStyle);
            }
        }

        // Initialize variables with default values or retrieve from local storage
            var partialCurrentview = getDataFromLocalStorage("partialCurrentview") || "DashBoard/_ProviderTableNew";
        var currentstatus = JSON.parse(getDataFromLocalStorage("currentstatus")) || [1];

        // Set initial values in local storage
        setDataInLocalStorage("partialCurrentview", partialCurrentview);
        setDataInLocalStorage("currentstatus", JSON.stringify(currentstatus));

        $('.triangle').css('display', 'none');
        //  $('#tranew').css('display', 'block').css('border-top', '20px solid #203f9a').css('color', 'white');


        filtertable(partialCurrentview, currentstatus, currentpage, pagesize);
        $('AdminTablePending').hide();
        $('AdminTableActive').hide();
        $('AdminTableConclude').hide();
      

        function updateActiveElement(id) {
            $(".status").removeClass("active");
            $(id).addClass("active");
            localStorage.setItem('activeElement', id); // Save active element to local storage
        }

        // Click event handlers
        $("#divnew").click(function () {
            updateActiveElement("#divnew");
        });

        $("#divpen").click(function () {
            updateActiveElement("#divpen");
        });

        $("#divact").click(function () {
            updateActiveElement("#divact");
        });

        $("#divcon").click(function () {
            updateActiveElement("#divcon");
        });

      

        // On page load, check if there's a previously active element in local storage and set i
        var activeElement = localStorage.getItem('activeElement');
        if (activeElement) {
            $(activeElement).addClass("active");
        }

        $(document).on("click", "#pagination a.page-link", function () {
            console.log("Pagination link clicked!");
            var id = $(this).attr("id");
            currentpage = $("#" + id).data("page");
            console.log("Current Page: " + currentpage);
            filtertable(partialCurrentview, currentstatus, currentpage, pagesize);
        });



        //-------------------on click functionality------------------------------

        $("#New").click(function () {
            currentstatus = [1];
            partialCurrentview = "DashBoard/_ProviderTableNew";
            setDataInLocalStorage("currentstatus", JSON.stringify(currentstatus));
            setDataInLocalStorage("partialCurrentview", partialCurrentview);
            $("#Search").val("");
            currentpage = 1;
            filtertable(partialCurrentview, currentstatus, currentpage, pagesize);
            // $('#textspan').text('(new)');
            // $('.triangle').css('display', 'none');
            // $('#tranew').css('display', 'block').css('border-top', '20px solid #203f9a').css('color', 'white');

            localStorage.setItem('textspanState', '(new)');
            localStorage.setItem('triangleDisplayState', 'none');
            localStorage.setItem('DisplayState', 'block');
            localStorage.setItem('BorderStyle', '20px solid #203f9a');
            localStorage.setItem('id', '#tranew');
            setStylesFromLocalStorage();
        });


        $("#Pending").click(function () {

            partialCurrentview = "DashBoard/_ProviderTablePending";
