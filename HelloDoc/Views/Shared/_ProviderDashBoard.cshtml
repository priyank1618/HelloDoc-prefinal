            localStorage.setItem('BorderStyle', '20px solid #da0f82');
            localStorage.setItem('id', '#tracon');
            setStylesFromLocalStorage();
        });

        setStylesFromLocalStorage();
        //-------------------on click functionality end------------------------------

        $('.form-select').on("change", function () {
            currentpage = 1;
            filtertable(partialCurrentview, currentstatus, currentpage, pagesize)
        });

        // -------------filter btn-----------------------------
        $('.filter-btn').click(function () {
            currentpage = 1;
            $('.filter-btn').removeClass('active')
            $(this).addClass('active')
            filtertable(partialCurrentview, currentstatus, currentpage, pagesize)

        });
        // -------------filter btn end -----------------------------

        // -------on search value--------------------
        $("#Search").on("input", function () {
            currentpage = 1;
            filtertable(partialCurrentview, currentstatus, currentpage, pagesize);
        });
        // -------on search value end---------------------

        function filtertable(partialName, currentstatus, currentpage, pagesize) {
            $.get('/ProviderDashBoard/CheckSession', function (session) {


                if (session.sessionExists) {
                    var SearchValue = $("#Search").val();
                    var Filterselect = $('.filter-btn.active').data('value');

                    console.log(Filterselect);
                    var selectopt = $(".form-select").find(':selected');
                    var selectvalue = selectopt.data('value');

                    var region = $("#Region").val();


                    if (SearchValue == "" && Filterselect == " ") {
                        filtertable(partialCurrentview, currentstatus, currentpage, pagesize);
                    }

                    else {
                        $.ajax({
                            type: "GET",
                            url: '@Url.Action("SearchPatient", "ProviderDashBoard")',
                            traditional: true,
                            data: {
                                SearchValue: SearchValue, Filterselect: Filterselect, selectvalue: selectvalue
                                , partialName: partialName, currentstatus: currentstatus, currentpage: currentpage, pagesize: pagesize
                            },
                            success: function (data) {
                                $('#PartialViews').html(data);
                                console.log(data)
                            },

                            error: function (error) {
                                console.error(" ", error);
                            }
                        });
                    }
                }

                else {
                    window.location.href = '/Login/Patient_login';
                }
            });

